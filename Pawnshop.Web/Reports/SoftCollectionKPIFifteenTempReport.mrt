        <Parameters isList="true" count="0" />
        <SqlCommand>DECLARE 
@datetimeNow DATE = CAST(dbo.GETASTANADATE() AS DATE)

IF (@year = 2022 AND @months = 12)
SELECT 
	@beginDate = DATEFROMPARTS(2022, 12, 1),
		@endDate = DATEFROMPARTS(2022, 12, 19)
ELSE IF (@year &lt; 2022 OR (@year = 2022 AND @months &lt; 12))
SELECT
	@beginDate = DATEFROMPARTS(@year, @months, 1),
		@endDate = EOMONTH(@beginDate)
		
	 ELSE
SELECT
	@beginDate = DATEFROMPARTS(IIF(@months-1 &lt;=0, @year-1,@year), IIF(@months-1&lt;=0,12,@months-1), 20),
		@endDate = DATEFROMPARTS(@year, @months, 19)

SET @endDate = IIF(@endDate &gt; @datetimeNow, @datetimeNow, @endDate)

DECLARE @superresult TABLE(
[Month] INT,
[Year] INT,
[Date] DATE,
TotalCount INT,
TotalCost BIGINT,
Count_1_8 INT,
Cost_1_8 BIGINT,
Percent_1_8 DECIMAL(16,4),
Count_8_15 INT,
Cost_8_15 BIGINT,
Percent_8_15 DECIMAL(16,4),
Count_hard INT,
Cost_hard BIGINT,
Percent_hard DECIMAL(16,4),
PercentCost BIGINT,
PenaltyCost BIGINT,
PercentAndPenaltyCost BIGINT
)

DECLARE @result TABLE(
Id INT,
DebtLeft DECIMAL(16,4),
GotDelayedInThisPeriod BIT,
SoftDelayDateStartsAt DATE,
DelayDayCount INT,
DebtCost DECIMAL(16,4),
PercentCost DECIMAL(16,4),
PenaltyCost DECIMAL(16,4),
HardDelayDateStartsAt DATE,
DebtLeftToHard DECIMAL(16,4),
PrepaymentCost DECIMAL(16,4)
)

DECLARE @DelayPeriods TABLE(
Id INT,
ClientId INT,
DelayStartsAt DATE,
DelayEndsAt DATE,
DelayDayCount INT
)

DECLARE @DelayPeriodsTEMP TABLE(
Id INT,
ClientId INT,
DelayStartsAt DATE,
DelayEndsAt DATE,
DelayDayCount INT
)

DECLARE @DelayTEMP TABLE(
Id INT,
ClientId INT,
DelayStartsAt DATE,
DelayEndsAt DATE,
DelayDayCount INT,
CountPrev INT
)

DECLARE @DelayActions TABLE(
Id INT,
ClientId INT,
DelayStartsAt DATE,
DelayEndsAt DATE
)

------АННУИТЕТ
INSERT INTO @DelayPeriodsTEMP(Id, ClientId, DelayStartsAt,DelayEndsAt,DelayDayCount)
SELECT 
	ContractId,
	ClientId,
	DATEADD(Day,1,[Date]) DelayStartsAt,
	IIF(ActualDate IS NULL, @datetimeNow, IIF(peny.Cost &gt; 0, @datetimeNow, ActualDate)) DelayEndsAt,
	DATEDIFF(Day,Date,IIF(ActualDate IS NULL, ISNULL(cps.Canceled, @datetimeNow), IIF(peny.Cost &gt; 0, @datetimeNow, ActualDate))) DelayDayCount
FROM ContractPaymentSchedule cps
JOIN Contracts c ON c.Id=cps.ContractId
LEFT JOIN LoanPercentSettings lps ON lps.Id = c.SettingId
OUTER APPLY (
SELECT SUM(ABS(dbo.GetAccountBalanceById (a.Id, cps2.ActualDate, DEFAULT, DEFAULT))) AS Cost
	FROM Accounts a
		JOIN AccountSettings acs ON acs.Id = a.AccountSettingId
		JOIN ContractPaymentSchedule cps2 ON cps2.Id = cps.Id
		WHERE a.ContractId = c.Id
		AND acs.Code IN ('PENY_ACCOUNT','PENY_PROFIT','PENY_ACCOUNT_OFFBALANCE','PENY_PROFIT_OFFBALANCE')) peny
WHERE ((cps.ActualDate IS NOT NULL AND DATEADD(Day,1,cps.Date) &gt;= @beginDate AND cps.Date &lt; @endDate) 
		OR (cps.ActualDate IS NULL AND cps.Date &lt; @endDate))
AND (cps.Canceled IS NULL OR cps.Canceled &gt;= @beginDate)
AND cps.DeleteDate IS NULL
AND c.DeleteDate IS NULL
AND c.collateralType=20
AND (c.PercentPaymentType &lt;&gt; 20 OR (c.PercentPaymentType = 40 AND lps.ScheduleType = 0))
AND c.Status&gt;=30
AND DATEDIFF(Day,cps.Date,IIF(cps.ActualDate IS NULL, ISNULL(cps.Canceled,@datetimeNow), ActualDate))&gt;0
AND c.BranchId IN ({branchIds})

INSERT INTO @DelayTEMP(Id, ClientId, DelayStartsAt,DelayEndsAt,DelayDayCount, CountPrev)
SELECT Id, ClientId ,DelayStartsAt,DelayEndsAt,DelayDayCount,
	DATEDIFF(DAY, LAG(ISNULL(DelayStartsAt, @datetimeNow)) OVER (PARTITION BY Id ORDER BY DelayStartsAt), LAG(ISNULL(DelayEndsAt, @datetimeNow)) OVER (PARTITION BY Id ORDER BY DelayEndsAt)) AS CountPrev
FROM @DelayPeriodsTEMP

INSERT INTO @DelayPeriods(Id, ClientId, DelayStartsAt, DelayEndsAt, DelayDayCount)
SELECT Id, ClientId, DelayStartsAt, DelayEndsAt, DelayDayCount from @DelayTEMP
WHERE ((CountPrev &gt; 0 AND CountPrev &lt; 30) OR CountPrev IS NULL)


--ДИСКРЕТ
INSERT INTO @DelayActions (Id, ClientId,DelayStartsAt, DelayEndsAt)
SELECT
	c.Id,
	c.ClientId,
	DATEADD(DAY,IIF(ca.ActionType=50,IIF(Locked=0,LoanPeriod-1,LoanPeriod),IIF(ca.ActionType=150,0, c.LoanPeriod)),IIF(ca.ActionType=50,CAST(c.ContractDate AS DATE),ca.Date)) AS DelayStartsAt,
	ISNULL(LEAD(ca.[Date]) OVER (PARTITION BY ca.ContractId ORDER BY ca.[Date]),ISNULL(buyout.Date, @datetimeNow)) AS DelayEndsAt
FROM Contracts c
OUTER APPLY (
SELECT Date, ContractId, Data, ActionType, DeleteDate FROM ContractActions ca
WHERE ca.ContractId = c.Id AND ca.ActionType IN (10,50,60,70,75,80,110,120,150)
) ca
OUTER APPLY (
SELECT Date FROM ContractActions buyout
WHERE buyout.ContractId = c.Id AND buyout.ActionType IN (20,30,40,90) AND buyout.DeleteDate IS NULL
) buyout
WHERE PercentPaymentType = 20
	AND c.Status&gt;=30
	AND c.collateralType=20
	AND c.ContractDate&lt;@enddate
	AND c.BranchId IN ({branchIds})

	AND ca.DeleteDate IS NULL



INSERT INTO @DelayPeriods(Id, ClientId, DelayStartsAt, DelayEndsAt, DelayDayCount)
SELECT 
	Id,
	ClientId,
	DATEADD(DAY,1,DelayStartsAt),
	DelayEndsAt,
	DATEDIFF(DAY,DelayStartsAt,DelayEndsAt) AS DelayDayCount
	FROM @DelayActions
WHERE 
DATEDIFF(DAY,DelayStartsAt,DelayEndsAt)&gt;0
	AND DelayStartsAt&lt;=@enddate
	AND DelayEndsAt &gt;=@begindate



INSERT INTO @result
(Id, DebtLeft, GotDelayedInThisPeriod, SoftDelayDateStartsAt, DelayDayCount, 
DebtCost, PercentCost, PenaltyCost, HardDelayDateStartsAt, PrepaymentCost)
SELECT
	d.Id,
	IIF(c.BuyOutDate IS NOT NULL AND c.LoanPeriod = 30, c.LoanCost, 
	IIF(c.LoanCost - carSum = 0, 
(SELECT SUM(car.Cost)
	FROM ContractActions ca, ContractActionRows car 
		WHERE ActionType IN (10,20,30,40,80,90,130)
		AND car.ActionId=ca.Id
		AND ca.DeleteDate IS NULL
		AND ca.ContractId = d.Id
		AND car.PaymentType=10
		AND ca.Date &gt;= @beginDate
		AND ca.Date &lt;= @endDate),
		c.LoanCost - ISNULL(carSum,0)))
		AS DebtLeft,
	IIF(DelayStartsAt BETWEEN @begindate AND @enddate,1,0) AS GotDelayedInThisPeriod,
	DelayStartsAt AS SoftDelayDateStartsAt,
	DelayDayCount,
	t.car10 AS DebtCost,
	t.car20 AS PercentCost,
	t.car30 AS PenaltyCost,
	IIF(DelayDayCount&gt;=16,DATEADD(DAY,16,DelayStartsAt),null) AS HardDelayDateStartsAt,
(SELECT SUM(TotalCost) FROM ContractActions ca WHERE ca.ContractId = c.Id AND ActionType = 100 AND [Date] BETWEEN DelayStartsAt AND ISNULL(IIF(DelayDayCount&gt;=16,DATEADD(DAY,16,DelayStartsAt),null),DATEADD(DAY,DelayDayCount,DelayStartsAt)))
FROM @DelayPeriods  d
JOIN Contracts c ON c.Id = d.Id
OUTER APPLY (
SELECT SUM(car.Cost) as carSum
	FROM ContractActions ca, ContractActionRows car 
		WHERE ActionType IN (10,20,30,40,80,90,130)
		AND car.ActionId=ca.Id
		AND ca.DeleteDate IS NULL
		AND ca.ContractId = d.Id
		AND car.PaymentType=10
		AND [Date] &lt;= @endDate) AS carSum
OUTER APPLY (
SELECT
	ISNULL(SUM(car10.Cost),0) AS car10,
	ISNULL(SUM(car20.Cost),0) AS car20,
	ISNULL(SUM(car30.Cost),0) AS car30
	FROM ContractActions ca
OUTER APPLY (select SUM(distinct Cost) AS Cost FROM ContractActionRows car WHERE PaymentType IN (10,15) AND car.ActionId = ca.Id) car10
OUTER APPLY (select SUM(distinct Cost) AS Cost FROM ContractActionRows car WHERE PaymentType IN (20,25) AND car.ActionId = ca.Id) car20
OUTER APPLY (select SUM(distinct Cost) AS Cost FROM ContractActionRows car WHERE PaymentType IN (30,31,32) AND car.ActionId = ca.Id) car30
WHERE ActionType IN (10,20,30,40,80,90,130)
	AND ContractId=d.Id 
	AND DeleteDate IS NULL
	AND Date&gt;=DelayStartsAt
	AND Date&lt;DATEADD(DAY,16,DelayStartsAt)
	) AS t

WHERE DelayStartsAt&lt;=@enddate
AND DelayEndsAt &gt;=@begindate
AND DATEDIFF(DAY,DelayStartsAt,@begindate)&lt;=16

UPDATE @result SET DebtLeftToHard = IIF(HardDelayDateStartsAt IS NOT NULL, DebtLeft, NULL)

BEGIN

INSERT INTO @superresult
SELECT MONTH(@endDate) AS month,
YEAR(@endDate) AS year,
@endDate AS Date,
COUNT(*) AS TotalCount,
SUM(DebtLeft) AS TotalCost,
SUM(IIF(DebtLeftToHard IS NULL AND DelayDayCount BETWEEN 1 AND 8,IIF((DebtCost + PercentCost + PenaltyCost) &gt; 0,1,0),0)) AS Count_1_8,
SUM(IIF(DebtLeftToHard IS NULL AND DelayDayCount BETWEEN 1 AND 8,IIF((DebtCost + PercentCost + PenaltyCost) &gt; 0,DebtLeft,0),0)) AS Cost_1_8,
ROUND(SUM(IIF(DebtLeftToHard IS NULL AND DelayDayCount BETWEEN 
1 AND 8,IIF((DebtCost + PercentCost + PenaltyCost) &gt; 0,DebtLeft,0),0))/(SUM(DebtLeft)/100),2) AS Percent_1_8,
SUM(IIF(DebtLeftToHard IS NULL AND DelayDayCount BETWEEN 9 AND 15,IIF((DebtCost + PercentCost + PenaltyCost) &gt; 0,1,0),0)) AS Count_8_15,
SUM(IIF(DebtLeftToHard IS NULL AND DelayDayCount BETWEEN 9 AND 15,IIF((DebtCost + PercentCost + PenaltyCost) &gt; 0,DebtLeft,0),0)) AS Cost_8_15,
ROUND(SUM(IIF(DebtLeftToHard IS NULL AND DelayDayCount BETWEEN 9 AND 15,IIF((DebtCost + PercentCost + PenaltyCost) &gt; 0,DebtLeft,0),0))/(SUM(DebtLeft)/100),2) AS Percent_8_15,
SUM(IIF(DebtLeftToHard IS NOT NULL,1,0)) AS Count_hard,
SUM(IIF(DebtLeftToHard IS NOT NULL,DebtLeft,0)) AS Cost_hard,
round(SUM(IIF(DebtLeftToHard IS NOT NULL, DebtLeft, 0))/(SUM(DebtLeft)/100),2) AS Percent_hard,
SUM(IIF(DebtLeftToHard IS NULL,ISNULL(PercentCost,0),0)) AS PercentCost,
SUM(IIF(DebtLeftToHard IS NULL,ISNULL(PenaltyCost,0),0)) AS PenaltyCost,
SUM(IIF(DebtLeftToHard IS NULL,ISNULL(PenaltyCost,0)+ISNULL(PercentCost,0),0)) AS PercentAndPenaltyCost
	FROM @result r
WHERE SoftDelayDateStartsAt &gt;= @beginDate AND SoftDelayDateStartsAt &lt;= @endDate
END
SELECT * FROM @superresult</SqlCommand>
      </ConsolidateSoftCollectionReport>
