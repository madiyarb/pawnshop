        <Name>InsurancePolicy</Name>
        <NameInSource>ReportConnection</NameInSource>
        <Parameters isList="true" count="0" />
        <SqlCommand>DECLARE @accountId INT, @TASOnline INT
SELECT @accountId=Id from Accounts where code='3397.7'
SELECT @TASOnline = COUNT(*) FROM Groups WHERE Name LIKE '%TSO' AND Id != 530 AND Id IN ({ branchIds })

SELECT
	c.ContractNumber,
	c.ContractDate,
	CASE
		WHEN c.Status = 0 THEN N'Черновик'
		WHEN c.Status = 4 THEN N'Ожидает открытия для ПВ'
		WHEN c.Status = 5 THEN N'Ожидает первоначального взноса'
		WHEN c.Status = 7 THEN N'Ожидает согласования(для недвижимости)'
		WHEN c.Status = 8 THEN N'Согласован(для недвижимости)'
		WHEN c.Status = 10 THEN N'Оформление или переоформление позиций'
		WHEN c.Status = 11 THEN N'Позиция переоформлена'
		WHEN c.Status = 15 THEN N'Ожидает подписания договора'
		WHEN c.Status = 20 THEN N'Ожидает зачисления денег'
		WHEN c.Status = 24 THEN N'Ожидает подтверждения кассового ордера'
		WHEN c.Status = 25 THEN N'Отменен/отклонен'
		WHEN c.Status = 26 THEN N'Ожидает создания заявки в СК'
		WHEN c.Status = 27 THEN N'Ожидает отправки заявки в СК'
		WHEN c.Status = 29 THEN N'Подтвержден в СК'
		WHEN c.Status = 30 THEN N'Подписан'
		WHEN c.Status = 40 THEN N'Выкуплен'
		WHEN c.Status = 50 THEN N'Отправлен на реализацию'
		WHEN c.Status = 60 THEN N'Реализован'
		ELSE N'-'
	END AS ContractStatus,
	c.BuyoutDate,
	cl.FullName,
	cl.IdentityNumber,
	IIF(@TASOnline &gt; 0,g.DisplayName,go.DisplayName) AS Branch,
	cs.OrderCost,
	ic.InsuranceCost,
	icl.FullName AS InsuranceName,
	i.InsuranceAmount,
	i.InsurancePremium,
	i.StartDate,
	i.EndDate,
	i.PoliceNumber,
	i.SurchargeAmount,
	i.EsbdAmount,
	CAST(
		IIF(
		i.SurchargeAmount &lt;&gt; 0,
		IIF(c.ContractDate &lt; '2023-06-30', i.SurchargeAmount * 0.9, i.SurchargeAmount * 0.93),
		IIF(c.ContractDate &gt;= '2023-06-30', i.InsurancePremium * 0.9, i.InsurancePremium * 0.93)
		) AS decimal(16, 2)
		) AS AgentComission,
	CASE
		WHEN i.EsbdAmount = 0 THEN i.InsuranceAmount
		ELSE i.EsbdAmount
	END AS TotalInsuranceAmount,
	CASE
		WHEN ipr.Status = 0 THEN N'Создана'
		WHEN ipr.Status = 10 THEN N'Отправлена в СК'
		WHEN ipr.Status = 11 THEN N'Отправлен в СК на Отмену'
		WHEN ipr.Status = 20 THEN N'Подтверждена в СК'
		WHEN ipr.Status = 30 THEN N'Отказано в СК'
		WHEN ipr.Status = 31 THEN N'Отказ Отмены в СК'
		WHEN ipr.Status = 40 THEN N'Отменена'
		WHEN ipr.Status = 50 THEN N'Ошибка'
		WHEN ipr.Status = 60 THEN N'Завершена'
		WHEN ipr.Status = 70 THEN N'Аннулирован'
		ELSE N'-'
	END AS PolicyStatus,
	dv.Name,
	ipcr.Date AS ClosingDate
FROM
	InsurancePolicies i
	JOIN Contracts c ON i.ContractId = c.Id
	JOIN Clients cl ON c.ClientId = cl.Id
	JOIN Groups g ON g.Id = c.BranchId
	OUTER APPLY(
SELECT
	SUM(OrderCost) AS OrderCost
	FROM ContractActions ca
				JOIN ContractActionRows car ON car.ActionId=ca.Id
				JOIN CashOrders co ON co.Id=car.OrderId
			WHERE ca.DeleteDate IS NULL
				AND ca.ActionType = 50
				AND ca.ContractId = c.Id
				AND co.DeleteDate IS NULL
				AND (co.OrderType = 20 or co.OrderType = 30)
		) cs
	OUTER APPLY(
SELECT
	SUM(OrderCost) AS InsuranceCost
	FROM CashOrders
	WHERE
	DeleteDate IS NULL
				AND ContractId = c.Id
				AND CreditAccountId = @accountId
				AND ApproveStatus = 10
		) ic
	JOIN InsurancePoliceRequests ipr ON i.PoliceRequestId = ipr.Id
	JOIN Clients icl ON icl.Id=ipr.InsuranceCompanyId
	LEFT JOIN InsurancePoliciesClosingReasons ipcr ON ipcr.PoliceRequestId = ipr.Id
	LEFT JOIN DomainValues dv ON ipcr.ClosingReasonId = dv.Id
	LEFT JOIN Dogs.ContractAdditionalInfo cai ON cai.Id=c.Id
	LEFT JOIN Groups go ON go.Id = cai.SelectedBranchId
WHERE
	i.StartDate BETWEEN @beginDate
	AND @endDate
	AND (
		i.DeleteDate IS NULL
		OR (
			ipr.DeleteDate IS NULL
			AND ipr.[Status] = 40
			AND ipcr.ClosingReasonId IS NOT NULL
			AND ipcr.DeleteDate IS NULL
		)
	)
	AND c.DeleteDate IS NULL
	AND cl.DeleteDate IS NULL
	AND ((@TASOnline = 0 AND c.BranchId IN ({ branchIds })) 
		OR (@TASOnline &gt; 0 AND c.BranchId = 530 AND cai.SelectedBranchId IN ({ branchIds })))
ORDER BY cl.IdentityNumber ASC</SqlCommand>
      </InsurancePolicy>
