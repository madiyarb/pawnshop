          <value>GoodsDescription,System.String</value>
          <value>DebtCost,System.Decimal</value>
          <value>DebtLeft,System.Decimal</value>
          <value>BranchName,System.String</value>
          <value>PercentPaymentType,System.Int32</value>
          <value>DelayDayCount,System.Int32</value>
          <value>LoanProfit,System.Decimal</value>
          <value>PenaltyProfit,System.Decimal</value>
          <value>DisplayStatus,System.Int32</value>
          <value>TransferedTo,System.String</value>
