        <Name>DiscountReport</Name>
        <NameInSource>ReportConnection</NameInSource>
        <Parameters isList="true" count="0" />
        <SqlCommand>SELECT
	c.ContractNumber,
	c.CollateralType,
	c.ContractDate,
	ISNULL(JSON_VALUE(c.ContractData, '$.Client.Fullname'), cl.FullName) as ClientName,
	c.LoanCost,
	c.MaturityDate,
	ca.ActionType,
	ca.[Date] as ActionDate,
	car.PaymentType,
	car.OriginalPercent,
	car.[Percent],
	c.LoanCost * (car.OriginalPercent / 100) * car.Period as OriginalCost,
	car.Cost,
	u.Fullname as AuthorName,
	g.DisplayName
	FROM ContractActionRows car
JOIN ContractActions ca ON car.ActionId = ca.Id
JOIN Contracts c ON ca.ContractId = c.Id
JOIN Users u ON ca.AuthorId = u.Id
LEFT JOIN Groups g ON g.Id=c.BranchId
JOIN Clients cl ON c.ClientId = cl.Id
LEFT JOIN ContractTransfers ct ON ct.ContractId = c.Id 
WHERE (@branchId=0 OR c.BranchId = @branchId)
	AND ca.[Date] BETWEEN @beginDate AND @endDate
	AND car.PaymentType IN (20, 30)
	AND ca.ActionType IN (10, 20, 30, 40)
	AND car.OriginalPercent &lt;&gt; car.[Percent]
    AND c.Status &lt;&gt; 0
    AND ct.TransferDate IS NULL
ORDER BY DisplayName, ContractDate</SqlCommand>
      </DiscountReport>
