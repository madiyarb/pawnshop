        <Parameters isList="true" count="0" />
        <SqlCommand>DECLARE @result TABLE(
Id INT,
DebtLeft DECIMAL(10,2),
GotDelayedInThisPeriod BIT,
SoftDelayDateStartsAt DATE,
DisplayStatus INT,
DelayDayCount INT,
DebtCost INT,
PercentCost INT,
PenaltyCost INT,
HardDelayDateStartsAt DATE,
DebtLeftToHard DECIMAL(10,2),
PrepaymentCost INT
)

DECLARE @DelayPeriods TABLE(
Id INT,
ClientId INT,
DelayStartsAt DATE,
DelayEndsAt DATE,
DelayDayCount INT
)

DECLARE @DelayPeriodsTEMP TABLE(
Id INT,
ClientId INT,
DelayStartsAt DATE,
DelayEndsAt DATE,
DelayDayCount INT
)

DECLARE @DelayTEMP TABLE(
Id INT,
DelayDayCount INT
)

------АННУИТЕТ
INSERT INTO @DelayPeriodsTEMP(Id, ClientId, DelayStartsAt, DelayEndsAt, DelayDayCount)
SELECT 
	ContractId,
	ClientId,
	[Date] DelayStartsAt,
	IIF(ActualDate IS NULL, CAST(dbo.GETASTANADATE() AS DATE),ActualDate) DelayEndsAt,
	DATEDIFF(Day,Date,IIF(ActualDate IS NULL, CAST(dbo.GETASTANADATE() AS DATE),ActualDate)) DelayDayCount
FROM ContractPaymentSchedule cps
JOIN Contracts c ON c.Id=cps.ContractId
WHERE Date&lt;=@enddate
AND (ActualDate IS NULL OR CAST(ActualDate AS DATE)&gt;=CAST(@begindate AS DATE))
AND Canceled IS NULL
AND cps.DeleteDate IS NULL
AND c.DeleteDate IS NULL
AND (@collateralType=0 OR c.collateralType=@collateralType)
AND c.Status&gt;0
AND DATEDIFF(Day,Date,IIF(ActualDate IS NULL, CAST(dbo.GETASTANADATE() AS DATE),ActualDate))&gt;0
AND c.BranchId IN ({branchIds})

INSERT INTO @DelayTEMP(Id, DelayDayCount)
SELECT Id, MAX(DelayDayCount) FROM @DelayPeriodsTEMP GROUP BY Id

INSERT INTO @DelayPeriods(Id, ClientId, DelayStartsAt, DelayEndsAt, DelayDayCount)
SELECT a.* FROM @DelayPeriodsTEMP a
LEFT JOIN @DelayTEMP z ON z.Id=a.Id
WHERE a.DelayDayCount=z.DelayDayCount

------ДИСКРЕТ
INSERT INTO @DelayPeriods(Id, ClientId, DelayStartsAt, DelayEndsAt, DelayDayCount)
SELECT 
	Id,
	ClientId,
	DelayStartsAt,
	DelayEndsAt,
	DATEDIFF(DAY,DelayStartsAt,DelayEndsAt) AS DelayDayCount
	FROM
(SELECT
	c.Id,
	c.ClientId,
	ca.Date,
	DATEADD(DAY,IIF(ca.ActionType=50,IIF(Locked=0,LoanPeriod-1,LoanPeriod),JSON_VALUE(ca.Data,'$.ProlongPeriod')),IIF(ca.ActionType=50,CAST(c.ContractDate AS DATE),ca.Date)) AS DelayStartsAt,

	ISNULL(LEAD(ca.[Date]) OVER (PARTITION BY ca.ContractId ORDER BY ca.[Date]),ISNULL(buyout.Date,dbo.GETASTANADATE())) as DelayEndsAt
FROM Contracts c
LEFT JOIN ContractActions ca on ca.ContractId=c.Id
LEFT JOIN ContractActions buyout on buyout.ContractId=c.Id AND buyout.ActionType IN (20,30,40,90,100)
WHERE PercentPaymentType = 20
	AND c.Status&gt;0
	AND ca.DeleteDate IS NULL
	AND (@collateralType=0 OR c.collateralType=@collateralType)
	AND ca.ActionType NOT IN (20,30,40,90,100)
	AND c.ContractDate&lt;@enddate
	AND c.BranchId IN ({branchIds})
	)
AS t
WHERE DATEDIFF(DAY,DelayStartsAt,DelayEndsAt)&gt;0
	AND DelayStartsAt&lt;=@enddate
	AND DelayEndsAt &gt;=@begindate
	

INSERT INTO @result
(Id, DebtLeft, GotDelayedInThisPeriod, SoftDelayDateStartsAt, DisplayStatus, DelayDayCount, 
DebtCost, PercentCost, PenaltyCost, HardDelayDateStartsAt, PrepaymentCost)
SELECT
	d.Id,
	c.LoanCost - ISNULL((SELECT SUM(car.Cost)
		FROM ContractActions ca
		LEFT JOIN ContractActionRows car ON car.ActionId=ca.Id
		WHERE ActionType IN (10,20,30,40,80,90)
		AND ca.DeleteDate IS NULL
		AND car.PaymentType=10
		AND ContractId=d.Id
		AND [Date]&lt;=DelayStartsAt),0) as DebtLeft,
	IIF(DelayStartsAt BETWEEN @begindate AND @enddate,1,0) AS GotDelayedInThisPeriod,
	DelayStartsAt AS SoftDelayDateStartsAt,
	ISNULL(t.ActionType,500) AS DisplayStatus,
	DelayDayCount,
	t.car10 AS DebtCost,
	t.car20 AS PercentCost,
	t.car30 AS PenaltyCost,
	IIF(DelayDayCount&gt;=6,DATEADD(DAY,6,DelayStartsAt),null) as HardDelayDateStartsAt,
(SELECT SUM(TotalCost) FROM ContractActions ca WHERE ca.ContractId = c.Id AND ActionType = 100 AND [Date] BETWEEN DelayStartsAt AND ISNULL(IIF(DelayDayCount&gt;=6,DATEADD(DAY,6,DelayStartsAt),null),DATEADD(DAY,DelayDayCount,DelayStartsAt)))
FROM @DelayPeriods  d
JOIN Contracts c ON c.Id = d.Id
OUTER APPLY (SELECT TOP 1
	ca.Id,
	ca.ActionType,
	ISNULL(SUM(car10.Cost),0) AS car10,
	ISNULL(SUM(car20.Cost),0) AS car20,
	ISNULL(SUM(car30.Cost),0) AS car30
FROM ContractActions ca
LEFT JOIN ContractActionRows car10 ON car10.ActionId=ca.Id AND car10.PaymentType=10
LEFT JOIN ContractActionRows car20 ON car20.ActionId=ca.Id AND car20.PaymentType=20
LEFT JOIN ContractActionRows car30 ON car30.ActionId=ca.Id AND car30.PaymentType=30
WHERE ActionType IN (10,20,30,40,80,90)
	AND ContractId=d.Id 
	AND DeleteDate IS NULL
	AND Date&gt;=DelayStartsAt
	AND Date&lt;DATEADD(DAY,6,DelayStartsAt)
Group by ca.Id,ca.ActionType) as t

WHERE DelayStartsAt&lt;=@enddate
AND DelayEndsAt &gt;=@begindate
AND DATEDIFF(DAY,DelayStartsAt,@begindate)&lt;=6

UPDATE @result SET DebtLeftToHard = IIF(HardDelayDateStartsAt IS NOT NULL, DebtLeft, NULL)


SELECT br.DisplayName as BranchName,total.TotalSum,soft.*,hard.*
FROM Groups br
OUTER APPLY(
SELECT 
	SUM(DebtLeft) AS TotalSum

	FROM @result r
JOIN Contracts c ON c.Id=r.Id
JOIN Groups g on g.Id=c.BranchId
WHERE r.DelayDayCount&lt;6 and g.Id=br.Id
GROUP BY g.DisplayName) as total
OUTER APPLY(
SELECT 
	Count(*) AS SoftCountContracts,
	r.DelayDayCount,
	SUM(DebtCost) AS DebtCost,
	SUM(PercentCost) AS PercentCost,
	SUM(PenaltyCost) AS PenaltyCost,
	SUM(PrepaymentCost) AS PrepaymentCost

	FROM @result r
JOIN Contracts c ON c.Id=r.Id
JOIN Groups g on g.Id=c.BranchId
WHERE r.DelayDayCount&lt;6 and g.Id=br.Id
GROUP BY g.DisplayName,r.DelayDayCount) as soft
OUTER APPLY(
SELECT 
	SUM(DebtLeftToHard) AS DebtLeftToHard,
	Count(*) AS HardCountContracts

	FROM @result r
JOIN Contracts c ON c.Id=r.Id
JOIN Groups g on g.Id=c.BranchId
WHERE r.DelayDayCount&gt;=6 and g.Id=br.Id
GROUP BY g.DisplayName) as hard
WHERE Id IN ({branchIds})
ORDER BY DisplayName</SqlCommand>
