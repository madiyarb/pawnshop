        <Name>SplitProfitReport</Name>
        <NameInSource>ReportConnection</NameInSource>
        <Parameters isList="true" count="0" />
        <SqlCommand>DECLARE @t TABLE (
    BranchName NVARCHAR(MAX),
    ContractId INT,
    ContractNumber NVARCHAR(MAX),
    ContractDate DATETIME,
    ClientFullName NVARCHAR(MAX),
    LoanCost MONEY,
    TransferDate DATETIME,
    ActionCost MONEY,
    TasCost MONEY,
    SfkCost MONEY
)

DECLARE @BranchName NVARCHAR(MAX)
DECLARE @ContractId INT
DECLARE @ContractNumber NVARCHAR(MAX)
DECLARE @ContractDate DATETIME
DECLARE @ClientFullName NVARCHAR(MAX)
DECLARE @LoanCost MONEY
DECLARE @LoanPercent DECIMAL(7, 4)
DECLARE @TransferDate DATETIME
DECLARE @BackTransferDate DATETIME

DECLARE cur CURSOR FAST_FORWARD READ_ONLY LOCAL FOR
SELECT 
	b.DisplayName as BranchName,
	c.Id as ContractId,
	c.ContractNumber,
	c.ContractDate,
	ISNULL(JSON_VALUE(c.ContractData, '$.Client.Fullname'), cl.FullName) as ClientFullName,
	c.LoanCost,
	c.LoanPercent,
	ct.TransferDate,
	ct.BackTransferDate
	FROM Contracts c
    JOIN Groups b ON c.BranchId = b.Id
	JOIN Clients cl on c.ClientId = cl.Id
	JOIN ContractTransfers ct ON c.id = ct.ContractId
    WHERE c.DeleteDate IS NULL
        AND ((@branchId &gt; 0 AND c.BranchId = @branchId) OR (@branchId = 0 AND c.BranchId IN (
SELECT rightGroup.Id
	FROM MemberRelations as relation
            JOIN Members AS rightMember ON rightMember.Id = relation.RightMemberId
            LEFT JOIN MemberRelations AS sourceRelation ON sourceRelation.Id = relation.SourceId
            LEFT JOIN Members AS sourceMember ON sourceMember.Id = sourceRelation.RightMemberId
            JOIN Groups AS rightGroup ON rightGroup.Id = rightMember.Id
            WHERE relation.LeftMemberId = @userId
                AND rightMember.Locked = 'False'
                AND rightGroup.[Type] = 20
                AND (sourceMember.Locked = 'False' OR sourceMember.Locked IS NULL)
        )))

OPEN cur
FETCH NEXT FROM cur INTO
    @BranchName,
    @ContractId,
    @ContractNumber,
    @ContractDate,
    @ClientFullName,
    @LoanCost,
    @LoanPercent,
    @TransferDate,
	@BackTransferDate
WHILE @@FETCH_STATUS = 0
BEGIN
    DECLARE @ActionCost MONEY = 0
    DECLARE @TasCost MONEY = 0
    DECLARE @SfkCost MONEY = 0

SELECT @ActionCost = SUM(ca.TotalCost)
    FROM ContractActions ca
    WHERE ca.DeleteDate IS NULL
        AND ca.ContractId = @ContractId
        AND ca.ActionType IN (10, 20, 30, 40)
        AND ca.Date BETWEEN @TransferDate AND @endDate

    IF @BackTransferDate is not null
	BEGIN
set  @ActionCost = @ActionCost + 
(select ca.TotalCost from ContractActions ca
	where ca.ContractId = @ContractId
		AND ca.DeleteDate IS NULL
        AND ca.ActionType = 75
		AND ca.Date BETWEEN @TransferDate AND @endDate
	) 
    end
IF @ActionCost IS NULL SET @ActionCost = 0

	

    DECLARE @ActionDate DATETIME
    DECLARE @ActionType INT
SELECT TOP 1 @ActionType = ca.ActionType, @ActionDate = ca.Date
    FROM ContractActions ca
    WHERE ca.DeleteDate IS NULL
        AND ca.ContractId = @ContractId
        AND ca.ActionType IN (10, 50)
        AND ca.Date &lt; @TransferDate
    ORDER BY ca.Date DESC

    IF @ActionDate IS NOT NULL
    BEGIN
        DECLARE @DayCount INT = 0
SET @DayCount = DATEDIFF(DAY, @ActionDate, @TransferDate)
IF @ActionType = 50 SET @DayCount = @DayCount + 1
IF @DayCount &gt; 30 SET @DayCount = 30
SET @TasCost = (@LoanCost * @LoanPercent / 100) * @DayCount
    END

IF @ActionCost = 0 SET @TasCost = 0
SET @SfkCost = @ActionCost - @TasCost

INSERT INTO @t ( BranchName, ContractId, ContractNumber, ContractDate, ClientFullName, LoanCost, TransferDate, ActionCost, TasCost, SfkCost )
    VALUES ( @BranchName, @ContractId, @ContractNumber, @ContractDate, @ClientFullName, @LoanCost, @TransferDate, @ActionCost, @TasCost, @SfkCost )
    
    FETCH NEXT FROM cur INTO
        @BranchName,
        @ContractId,
        @ContractNumber,
        @ContractDate,
        @ClientFullName,
        @LoanCost,
        @LoanPercent,
        @TransferDate,
		@BackTransferDate
END

CLOSE cur
DEALLOCATE cur

SELECT *
	FROM @t
ORDER BY BranchName, ContractNumber</SqlCommand>
