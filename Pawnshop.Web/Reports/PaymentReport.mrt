				<Parameters isList="true" count="0" />
				<SqlCommand>
					DECLARE
					@ContactTypeId INT

					SELECT @ContactTypeId = id from DomainValues where DomainCode = N'CONTACT_TYPE' AND Code = N'MOBILE_PHONE' AND DeleteDate IS NULL
					DECLARE @t TABLE (
					Id INT,
					PercentPaymentType INT,
					ContractNumber NVARCHAR(500),
					CollateralType INT,
					ContractDate DATETIME,
					MaturityDate DATETIME,
					NextPaymentDate DATETIME,
					Payment DECIMAL(16,2),
					LoanCost INT,
					DebtLeftCost DECIMAL(16,2),
					ClientName NVARCHAR(500),
					ClientIdentityNumber NVARCHAR(20),
					PositionName NVARCHAR(MAX),
					IsAnnuity BIT,
					HasCasco BIT,
					LoanPercent DECIMAL(16,2),
					PrePaymentCost DECIMAL(16,2),
					ContractNote NVARCHAR(MAX),
					ContractDiscount NVARCHAR(MAX),
					ContractPostponement NVARCHAR(MAX),
					RealPhone NVARCHAR(MAX),
					BranchId int,
					AuthorId int
					);
					DECLARE @CurrentTime DATETIME = dbo.GETASTANADATE();
					INSERT INTO @t (Id, PercentPaymentType, ContractNumber, CollateralType, ContractDate, MaturityDate, Payment, LoanCost, DebtLeftCost,
					ClientName, ClientIdentityNumber, IsAnnuity, LoanPercent, PrePaymentCost, ContractNote, ContractDiscount, ContractPostponement, RealPhone,
					BranchId, AuthorId)
					SELECT
					c.Id,
					c.PercentPaymentType,
					c.ContractNumber,
					c.CollateralType,
					c.ContractDate,
					c.MaturityDate,
					(SELECT top 1 ISNULL((DebtCost+PercentCost),0)  FROM ContractPaymentSchedule WHERE ContractId=c.Id AND DeleteDate IS NULL AND ActualDate IS NULL AND Canceled IS NULL),
					c.LoanCost,
					ISNULL(dbo.GetAccountBalanceById(acc_ACCOUNT.Id, @endDate, DEFAULT, DEFAULT),0)*(-1),
					cl.FullName,
					cl.IdentityNumber,
					0 AS IsAnnuity,
					c.LoanPercent * 30,
					dbo.GetAccountBalanceById(acc.Id, @endDate, DEFAULT, DEFAULT),
					(SELECT CONCAT(u.FullName,N': ', n.Note,CHAR(10))
					FROM ContractNotes n
					JOIN Users u ON u.Id=n.AuthorId
					WHERE ContractId=c.Id
					ORDER BY n.Id DESC
					FOR XML PATH('')) AS ContractNote,
					(SELECT CONCAT(N'-',u.FullName,N': ', pd.Note,CHAR(10))
					FROM ContractDiscounts cd
					JOIN Users u ON u.Id=cd.AuthorId
					JOIN PersonalDiscounts pd ON pd.Id=cd.PersonalDiscountId
					WHERE cd.ContractId=c.Id AND cd.DeleteDate IS NULL AND @CurrentTime BETWEEN cd.BeginDate AND cd.EndDate
					ORDER BY cd.Id DESC
					FOR XML PATH('')) AS ContractDiscount,
					(SELECT CONCAT(N'-',u.FullName,N': ', p.Name,CHAR(10))
					FROM ContractPostponements cp
					JOIN Users u ON u.Id=cp.AuthorId
					JOIN Postponements p ON p.Id=cp.PostponementId
					WHERE cp.ContractId=c.Id AND cp.DeleteDate IS NULL AND @CurrentTime&lt;=cp.Date
					ORDER BY cp.Id DESC
					FOR XML PATH('')) AS ContractPostponement,
					(SELECT CONCAT(N'',cc.Address,N': ',CHAR(10))
					FROM Clients cl
					JOIN ClientContacts cc ON cl.Id = cc.ClientId
					WHERE cl.Id = c.ClientId AND cl.DeleteDate IS NULL AND cc.DeleteDate IS NULL AND cc.ContactTypeId = @ContactTypeId
					ORDER BY cl.Id DESC
					FOR XML PATH('')) AS RealPhone,
					c.BranchId,
					c.AuthorId
					FROM Contracts c
					OUTER APPLY (SELECT TOP 1 a.Id FROM Accounts a JOIN AccountSettings acs ON a.AccountSettingId = acs.Id WHERE acs.Code = 'OVERDUE_ACCOUNT' AND a.ContractId = c.Id) OverdueAccount
					OUTER APPLY (SELECT TOP 1 a.Id FROM Accounts a JOIN AccountSettings acs ON a.AccountSettingId = acs.Id WHERE acs.Code = 'OVERDUE_PROFIT' AND a.ContractId = c.Id) OverdueProfit
					OUTER APPLY (SELECT ISNULL(dbo.GetAccountBalanceById(OverdueAccount.Id, @endDate, default, default),0) bal) overdueAccountBalance
					OUTER APPLY (SELECT ISNULL(dbo.GetAccountBalanceById(OverdueProfit.Id, @endDate, default, default),0) bal) overdueProfitBalance
					LEFT JOIN Clients cl on cl.Id=c.ClientId
					OUTER APPLY	(
					SELECT a.Id
					FROM Accounts a
					JOIN AccountSettings acs ON a.AccountSettingId = acs.Id
					WHERE a.ContractId = c.Id AND acs.Code = N'DEPO' AND a.DeleteDate IS NULL
					) acc
					OUTER APPLY	(
					SELECT a.Id
					FROM Accounts a
					JOIN AccountSettings acs ON a.AccountSettingId = acs.Id
					WHERE a.ContractId = c.Id AND acs.Code = N'ACCOUNT' AND a.DeleteDate IS NULL
					) acc_ACCOUNT
					WHERE c.DeleteDate IS NULL
					AND c.PercentPaymentType = 20
					AND c.[Status] = 30
					AND c.MaturityDate BETWEEN @beginDate AND @endDate
					AND c.BranchId IN ({branchIds})
					AND (@collateralType = 0 OR c.CollateralType = @collateralType)
					AND overdueAccountBalance.bal = 0
					AND overdueProfitBalance.bal = 0

					INSERT INTO @t (Id, PercentPaymentType, ContractNumber, CollateralType, ContractDate, MaturityDate, NextPaymentDate, Payment, LoanCost, DebtLeftCost,
					ClientName, ClientIdentityNumber, IsAnnuity, LoanPercent, PrePaymentCost, ContractNote, ContractDiscount, ContractPostponement, RealPhone,
					BranchId, AuthorId)
					SELECT
					c.Id,
					c.PercentPaymentType,
					c.ContractNumber,
					c.CollateralType,
					c.ContractDate,
					cps.Date AS MaturityDate,
					(SELECT TOP 1 Date FROM ContractPaymentSchedule where ContractId=c.Id AND c.DeleteDate IS NULL AND Date&gt;cps.Date ORDER BY Date) AS NextPaymentDate,
					cps.DebtCost+cps.PercentCost,
					c.LoanCost,
					ISNULL(dbo.GetAccountBalanceById(acc_ACCOUNT.Id, @endDate, DEFAULT, DEFAULT),0)*(-1),
					cl.FullName,
					cl.IdentityNumber,
					1 AS IsAnnuity,
					c.LoanPercent * 30,
					dbo.GetAccountBalanceById(acc.Id, @endDate, DEFAULT, DEFAULT),
					(SELECT CONCAT(u.FullName,N': ', n.Note,CHAR(10))
					FROM ContractNotes n
					JOIN Users u ON u.Id=n.AuthorId
					WHERE ContractId=c.Id
					ORDER BY n.Id DESC
					FOR XML PATH('')) AS ContractNote,
					(SELECT CONCAT(N'-',u.FullName,N': ', pd.Note,CHAR(10))
					FROM ContractDiscounts cd
					JOIN Users u ON u.Id=cd.AuthorId
					JOIN PersonalDiscounts pd ON pd.Id=cd.PersonalDiscountId
					WHERE cd.ContractId=c.Id AND cd.DeleteDate IS NULL AND @CurrentTime BETWEEN cd.BeginDate AND cd.EndDate
					ORDER BY cd.Id DESC
					FOR XML PATH('')) AS ContractDiscount,
					(SELECT CONCAT(N'-',u.FullName,N': ', p.Name,CHAR(10))
					FROM ContractPostponements cp
					JOIN Users u ON u.Id=cp.AuthorId
					JOIN Postponements p ON p.Id=cp.PostponementId
					WHERE cp.ContractId=c.Id AND cp.DeleteDate IS NULL AND @CurrentTime&lt;=cp.Date
					ORDER BY cp.Id DESC
					FOR XML PATH('')) AS ContractPostponement,
					(SELECT CONCAT(N'',cc.Address,N': ',CHAR(10))
					FROM Clients cl
					JOIN ClientContacts cc ON cl.Id = cc.ClientId
					WHERE cl.Id = c.ClientId AND cl.DeleteDate IS NULL AND cc.DeleteDate IS NULL AND cc.DeleteDate IS NULL AND cc.ContactTypeId = @ContactTypeId
					ORDER BY cl.Id DESC
					FOR XML PATH('')) AS RealPhone,
					c.BranchId,
					c.AuthorId
					FROM Contracts c
					OUTER APPLY (SELECT TOP 1 a.Id FROM Accounts a JOIN AccountSettings acs ON a.AccountSettingId = acs.Id WHERE acs.Code = 'OVERDUE_ACCOUNT' AND a.ContractId = c.Id) OverdueAccount
					OUTER APPLY (SELECT TOP 1 a.Id FROM Accounts a JOIN AccountSettings acs ON a.AccountSettingId = acs.Id WHERE acs.Code = 'OVERDUE_PROFIT' AND a.ContractId = c.Id) OverdueProfit
					OUTER APPLY (SELECT ISNULL(dbo.GetAccountBalanceById(OverdueAccount.Id, @endDate, default, default),0) bal) overdueAccountBalance
					OUTER APPLY (SELECT ISNULL(dbo.GetAccountBalanceById(OverdueProfit.Id, @endDate, default, default),0) bal) overdueProfitBalance
					LEFT JOIN Clients cl on cl.Id=c.ClientId
					LEFT JOIN ContractPaymentSchedule cps on cps.ContractId=c.Id AND cps.deletedate is null AND cps.ActionId IS NULL
					OUTER APPLY	(
					SELECT a.Id
					FROM Accounts a
					JOIN AccountSettings acs ON a.AccountSettingId = acs.Id
					WHERE a.ContractId = c.Id AND acs.Code = N'DEPO' AND a.DeleteDate IS NULL
					) acc
					OUTER APPLY	(
					SELECT a.Id
					FROM Accounts a
					JOIN AccountSettings acs ON a.AccountSettingId = acs.Id
					WHERE a.ContractId = c.Id AND acs.Code = N'ACCOUNT' AND a.DeleteDate IS NULL
					) acc_ACCOUNT
					WHERE c.DeleteDate IS NULL
					AND c.PercentPaymentType IN (30, 31, 32, 40)
					AND c.[Status] = 30
					AND cps.Date BETWEEN @beginDate AND @endDate
					AND c.BranchId IN ({branchIds})
					AND (@collateralType = 0 OR c.CollateralType = @collateralType)
					AND overdueAccountBalance.bal = 0
					AND overdueProfitBalance.bal = 0

					DELETE FROM @t WHERE Id IN (
						SELECT ContractId FROM ClientDeferments
							WHERE DeleteDate IS NULL AND
								((Status = 20  AND StartDate &lt; dbo.GETASTANADATE() AND EndDate &gt;= dbo.GETASTANADATE())
								OR
								(Status = 10  AND StartDate &lt; dbo.GETASTANADATE()))
						)

					DECLARE @Id INT

					DECLARE cur CURSOR FOR SELECT Id FROM @t
					OPEN cur
					FETCH NEXT FROM cur INTO @Id
					WHILE @@FETCH_STATUS = 0
					BEGIN
					UPDATE @t
					SET PositionName = (
					SELECT TOP 1 (
					CASE WHEN c.CollateralType = 10 THEN p.Name + ' (' + ISNULL(JSON_VALUE(cp.PositionSpecific, '$.CollateralTotalWeight'), '') + ISNULL(JSON_VALUE(cp.PositionSpecific, '$.CollateralSpecificWeight'), '') + ISNULL(cp.Note, '') + ')'
					WHEN c.CollateralType = 20 THEN ISNULL(UPPER(car.Mark), '') + ISNULL(' ' + UPPER(car.Model), '') + CAST(car.ReleaseYear as NVARCHAR) + ISNULL(' ' + UPPER(car.Color), '') + ISNULL(' ' + UPPER(car.TransportNumber), '')
					WHEN c.CollateralType = 40 THEN ISNULL(UPPER(mac.Mark), '') + ISNULL(' ' + UPPER(mac.Model), '') + CAST(mac.ReleaseYear as NVARCHAR) + ISNULL(' ' + UPPER(mac.Color), '') + ISNULL(' ' + UPPER(mac.TransportNumber), '')
					ELSE p.Name + ISNULL(cp.Note, '')
					END
					) AS PositionName
					FROM ContractPositions cp
					JOIN Contracts c ON cp.ContractId = c.Id
					JOIN Positions p ON cp.PositionId = p.Id
					LEFT JOIN Cars car ON p.Id = car.Id
					LEFT JOIN Machineries mac ON p.Id = mac.Id
					LEFT JOIN Purities pur ON JSON_VALUE(cp.PositionSpecific, '$.PurityId') = pur.Id
					WHERE cp.ContractId = @Id
					)
					WHERE Id = @Id

					UPDATE @t
					SET HasCasco = ISNULL((
					SELECT TOP 1 1
					FROM Insurances i
					WHERE i.ContractId = @Id
					), 0)
					WHERE Id = @Id

					FETCH NEXT FROM cur INTO @Id
					END

					SELECT t.*, g.DisplayName as BranchName, u.FullName as Manager
					FROM @t t
					inner join
					Groups g
					on g.Id = t.BranchId
					inner join
					Users u
					on u.Id = t.AuthorId
					ORDER BY ContractDate
				</SqlCommand>
			</PaymentReport>
