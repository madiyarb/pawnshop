      <Redeemed Ref="3" type="Stimulsoft.Report.Dictionary.StiSqlSource" isKey="true">
        <Alias>Redeemed</Alias>
        <Columns isList="true" count="5">
          <value>Branch,System.String</value>
          <value>AVG_LoanCost,System.Int64</value>
          <value>AVG_LoanPercent,System.Decimal</value>
          <value>AVG_LoanPeriod,System.Int32</value>
          <value>COUNT_Contracts,System.Int32</value>
        </Columns>
        <CommandTimeout>30</CommandTimeout>
        <Dictionary isRef="1" />
        <Name>Redeemed</Name>
        <NameInSource>ReportConnection</NameInSource>
        <Parameters isList="true" count="0" />
        <SqlCommand>SELECT
	g.DisplayName as Branch,
	AVG(CAST(c.LoanCost AS BIGINT)) AS AVG_LoanCost,
	AVG(c.LoanPercent)*30 AS AVG_LoanPercent,
	AVG(IIF((c.MaturityDate&lt;GETDATE()),
		DATEDIFF(DAY, c.ContractDate, IIF((c.MaturityDate&gt;GETDATE()),GETDATE(),c.MaturityDate)),
		DATEDIFF(DAY, c.ContractDate, IIF((@endDate&gt;GETDATE()),GETDATE(),@endDate)))) as AVG_LoanPeriod,
	COUNT(*) AS COUNT_Contracts
FROM Contracts c
JOIN Groups g ON g.Id=c.BranchId
LEFT JOIN ContractActions ca on ca.ContractId=c.Id and ca.ActionType=20
WHERE c.ContractDate BETWEEN @beginDate AND @endDate
	AND c.DeleteDate IS NULL
	AND ca.Id IS NOT NULL
	AND c.BranchId IN ({branchIds})
GROUP BY g.DisplayName
ORDER BY 2 DESC</SqlCommand>
      </Redeemed>
