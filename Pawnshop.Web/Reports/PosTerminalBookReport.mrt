        <Name>Balance</Name>
        <NameInSource>ReportConnection</NameInSource>
        <Parameters isList="true" count="0" />
        <SqlCommand>DECLARE
@posPlan INT

--POS PLAN
SELECT @posPlan = dbo.GET_ACCOUNT_PLAN_ID_BY_CODE('POS_ACCOUNT', 'CONTRACTS_CRED', 'TERMS_ALL')
	
	SELECT 
	g.DisplayName as BranchName,
	SUM(CAST(ABS(dbo.GetAccountBalanceById (a.Id, @beginDate, DEFAULT, 0)) AS INT)) PosTerminalBeginPeriod, 
SUM(CAST(ABS(dbo.GetAccountBalanceById (a.Id, @endDate, DEFAULT, DEFAULT)) AS INT)) PosTerminalEndPeriod 
FROM Accounts a
JOIN Groups g ON a.BranchId = g.Id
WHERE AccountPlanId = @posPlan
AND BranchId = @branchId
GROUP BY g.DisplayName</SqlCommand>
      </Balance>
