      <PosTerminalBook Ref="3" type="Stimulsoft.Report.Dictionary.StiSqlSource" isKey="true">
        <Alias>PosTerminalBook</Alias>
        <Columns isList="true" count="6">
          <value>AccountCode,System.String</value>
          <value>CashIn,System.Int32</value>
          <value>CashOut,System.Int32</value>
          <value>ClientName,System.String</value>
          <value>OrderNumber,System.String</value>
          <value>ClientIdentityNumber,System.String</value>
        </Columns>
        <CommandTimeout>240</CommandTimeout>
        <Dictionary isRef="1" />
        <Key>c6b05b7781274a3ab8cc6f5de5739d42</Key>
        <Name>PosTerminalBook</Name>
        <NameInSource>ReportConnection</NameInSource>
        <Parameters isList="true" count="0" />
        <SqlCommand>DECLARE
@posPlan INT

--POS PLAN
SELECT @posPlan = dbo.GET_ACCOUNT_PLAN_ID_BY_CODE('POS_ACCOUNT', 'CONTRACTS_CRED', 'TERMS_ALL')

SELECT
	co.OrderNumber,
	(CASE WHEN c.FullName IS NULL THEN u.FullName ELSE c.FullName END) AS ClientName,
	(CASE WHEN c.IdentityNumber IS NULL THEN u.IdentityNumber ELSE c.IdentityNumber END) AS ClientIdentityNumber,
    (CASE WHEN da.AccountPlanId = @posPlan THEN ca.Code
		WHEN ca.AccountPlanId = @posPlan THEN da.Code END) AS AccountCode,
    (CASE WHEN da.AccountPlanId = @posPlan THEN co.OrderCost ELSE 0 END) AS CashIn,
    (CASE WHEN ca.AccountPlanId = @posPlan THEN co.OrderCost ELSE 0 END) AS CashOut
FROM CashOrders co
JOIN Accounts da ON co.DebitAccountId = da.Id
JOIN Accounts ca ON co.CreditAccountId = ca.Id
LEFT JOIN Clients c ON co.ClientId = c.Id
LEFT JOIN Users u ON co.UserId = u.Id
OUTER APPLY(
SELECT co2.Id, co2.DeleteDate FROM
	CashOrders co2
WHERE co2.StornoId = co.Id
) stornoOrder
WHERE co.OrderDate BETWEEN @beginDate AND @endDate
    AND co.DeleteDate IS NULL
	AND co.ApproveStatus = 10
    AND co.BranchId = @branchId
    AND (da.AccountPlanId = @posPlan OR ca.AccountPlanId = @posPlan)
	AND ((stornoOrder.Id IS NULL AND co.StornoId IS NULL) OR (stornoOrder.DeleteDate IS NOT NULL))
ORDER BY co.OrderDate</SqlCommand>
      </PosTerminalBook>
      <Balance Ref="4" type="Stimulsoft.Report.Dictionary.StiSqlSource" isKey="true">
        <Alias>Balance</Alias>
        <Columns isList="true" count="3">
          <value>BranchName,System.String</value>
          <value>PosTerminalBeginPeriod,System.Int64</value>
          <value>PosTerminalEndPeriod,System.Int64</value>
