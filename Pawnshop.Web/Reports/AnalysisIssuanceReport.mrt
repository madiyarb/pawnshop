      <AnalysisIssuanceReport Ref="3" type="Stimulsoft.Report.Dictionary.StiSqlSource" isKey="true">
        <Alias>AnalysisIssuanceReport</Alias>
        <Columns isList="true" count="13">
          <value>ContractNumber,System.String</value>
          <value>Issuance,System.String</value>
          <value>FullName,System.String</value>
          <value>IdentityNumber,System.String</value>
          <value>MobilePhone,System.String</value>
          <value>LoanCost,System.Int32</value>
          <value>ContractDate,System.DateTime</value>
          <value>Manager,System.String</value>
          <value>BranchName,System.String</value>
          <value>City,System.String</value>
          <value>DisplayStatus,System.String</value>
          <value>ActionDate,System.String</value>
          <value>ActionStatus,System.String</value>
        </Columns>
        <CommandTimeout>30</CommandTimeout>
        <Dictionary isRef="1" />
        <Key>69cc8aae0bc647b9aee091a27817a198</Key>
        <Name>AnalysisIssuanceReport</Name>
        <NameInSource>ReportConnection</NameInSource>
        <Parameters isList="true" count="0" />
        <SqlCommand>SELECT 
	con.ContractNumber AS ContractNumber,
	(CASE 
		 WHEN res.isMigrated = 1 THEN N'Мигрированный'
		 WHEN res.isPartialPayment = 1 THEN N'ЧДП'
		 WHEN res.isAddition = 1 THEN N'Добор'
		 WHEN res.isOld = 1 THEN N'Повторная выдача'
		 ELSE N'Новый клиент'
	END) AS Issuance,
	cln.FullName AS FullName,
	cln.IdentityNumber AS IdentityNumber,
	cln.MobilePhone AS MobilePhone,
	con.LoanCost AS LoanCost,
	CAST(con.ContractDate AS DATE) AS ContractDate,
	us.FullName AS Manager,
	gr.DisplayName AS BranchName,
	ISNULL(JSON_VALUE(gr.Configuration, '$.ContactSettings.City'),JSON_VALUE(org.Configuration, '$.ContactSettings.City')) as City,
	(CASE 
        WHEN con.DeleteDate IS NOT NULL THEN N'Удален'
        WHEN con.Status = 0 THEN N'Новый'
WHEN con.Status = 30 AND (SELECT COUNT(*) FROM ContractPaymentSchedule WHERE ContractId=con.Id AND Canceled IS NULL AND DeleteDate IS NULL AND ActionId IS NULL AND Date&lt; CONVERT(DATE, @endDate))&gt;0 THEN N'Просрочен'
        WHEN con.Status = 30 AND con.MaturityDate &gt;= CONVERT(DATE, @endDate) AND con.ProlongDate IS NULL THEN N'Открыт'
        WHEN con.Status = 30 AND con.MaturityDate &lt; CONVERT(DATE, @endDate) THEN N'Просрочен'
        WHEN con.Status = 30 AND con.MaturityDate &gt;= CONVERT(DATE, @endDate) AND con.ProlongDate IS NOT NULL THEN N'Продлен'
        WHEN con.Status = 40 THEN N'Выкуплен'
        WHEN con.Status = 50 THEN N'Отправлен на реализацию'
        ELSE N'Новый'
    END) AS DisplayStatus,
	(CASE 
        WHEN last_action.ActionType = 10 THEN CONVERT(NVARCHAR, CAST(last_action.Date AS DATE))
        WHEN last_action.ActionType = 20 THEN CONVERT(NVARCHAR, CAST(last_action.Date AS DATE))
        WHEN last_action.ActionType = 40 THEN CONVERT(NVARCHAR, CAST(last_action.Date AS DATE))
        WHEN last_action.ActionType = 90 THEN CONVERT(NVARCHAR, CAST(last_action.Date AS DATE))
        ELSE N'-'
    END) AS ActionDate,
	(CASE 
        WHEN last_action.ActionType = 10 THEN N'Продление'
        WHEN last_action.ActionType = 20 THEN N'Выкуп'
        WHEN last_action.ActionType = 40 THEN N'Частичное погашение'
        WHEN last_action.ActionType = 90 THEN N'Добор'
        ELSE N'-'
    END) AS ActionStatus

FROM
	(
SELECT 
	cont.Id,
	IIF(cont.AuthorId=1 AND cont.locked=1 AND LEN(cont.note)&gt;0,1,0) as isMigrated,
	IIF(COUNT(count_cont.Id)&gt;0,1,0) as isOld,
	IIF(cont.Locked=1 AND cont.PartialPaymentParentId&gt;0,1,0) AS isPartialPayment,
	IIF(cont.Locked=1 AND cont.PartialPaymentParentId IS NULL,1,0) AS isAddition
	FROM Contracts cont 
		LEFT JOIN Contracts count_cont ON count_cont.ClientId = cont.ClientId AND count_cont.ContractDate&lt;=cont.ContractDate AND count_cont.Id!=cont.Id
		JOIN ContractActions cont_act ON cont_act.ContractId = cont.Id AND cont_act.ActionType = 50 
	WHERE cont.DeleteDate is null
		AND cont.ContractDate &gt;= @beginDate
		AND cont.ContractDate &lt;= @endDate
AND cont.BranchId IN ({branchIds})
	GROUP BY cont.Id,cont.ContractNumber, cont.AuthorId, cont.locked, cont.note,cont.PartialPaymentParentId
	) AS res
	LEFT JOIN Contracts con ON con.Id = res.Id
	LEFT JOIN Clients cln ON cln.Id = con.ClientId
	
	LEFT JOIN Groups gr ON gr.Id = con.BranchId
	LEFT JOIN Users us ON us.Id = con.AuthorId
	LEFT JOIN Members mem ON mem.Id = gr.Id
	LEFT JOIN Organizations org ON org.Id=mem.OrganizationId
	OUTER APPLY(
SELECT TOP 1 * 
		FROM ContractActions ca
		WHERE ContractId = con.Id 
			AND ca.DeleteDate IS NULL
			AND ca.ActionType!=100
		ORDER BY ID DESC
	) AS last_action
ORDER BY 9,1</SqlCommand>
      </AnalysisIssuanceReport>
